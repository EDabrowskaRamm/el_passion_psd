{
  "name": "starter",
  "scripts": {
    "cm": "git-cz",
    "dev": "BABEL_ENV=all next",
    "start": "BABEL_ENV=all next build && next start",
    "export": "BABEL_ENV=all next build && next export",
    "lint": "eslint ./pages ./components",
    "test": "jest --no-cache --watch",
    "test-ci": "jest --coverage",
    "analyze": "cross-env ANALYZE=1 next build",
    "commitmsg": "validate-commit-msg",
    "prepush": "npm run lint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest-config/setupTests.js"
    ]
  },
  "author": "Adam Borowski <adam@adamboro.com>",
  "dependencies": {
    "babel-loader": "^7.1.2",
    "babel-plugin-root-import": "^5.1.0",
    "css-loader": "^0.28.8",
    "cssnano": "^3.10.0",
    "glob": "^7.1.2",
    "isomorphic-unfetch": "^2.0.0",
    "next": "^4.2.3",
    "node-sass": "^4.7.2",
    "normalize.css": "^7.0.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^2.0.10",
    "raw-loader": "^0.5.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "commitizen": "^2.9.6",
    "cross-env": "^5.1.3",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "jest-cli": "^22.1.2",
    "validate-commit-msg": "^2.14.0",
    "webpack-bundle-analyzer": "^2.9.2"
  }
}
