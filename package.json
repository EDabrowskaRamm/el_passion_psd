{
  "version": "1.1.0",
  "name": "spark",
  "scripts": {
    "cm": "git-cz",
    "dev": "cross-env BABEL_ENV=all next",
    "start": "npm run build && next start",
    "export": "npm run build-export && cross-env STATIC_EXPORT=1 next export && node scripts/after-export.js",
    "build-export": "npm run build && node scripts/static-hash.js && cross-env BABEL_ENV=all STATIC_EXPORT=1 next build",
    "build": "cross-env BABEL_ENV=all next build",
    "lint": "eslint ./pages ./components",
    "test": "jest --no-cache --watch",
    "test-ci": "jest --coverage",
    "storybook": "cross-env BABEL_ENV=all start-storybook -s ./ -p 9001",
    "storybook:build": "cross-env BABEL_ENV=all build-storybook -o styleguide",
    "release": "corp-semantic-release",
    "postversion": "git add package-lock.json",
    "analyze": "cross-env BABEL_ENV=all ANALYZE=1 next build",
    "commitmsg": "validate-commit-msg",
    "prepush": "npm run lint",
    "clean": "rm -rf .next out static/global.css src/static-hash.json",
    "new": "node scripts/new-project.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest-config/setupTests.js"
    ]
  },
  "author": "Adam Borowski <adam@adamboro.com>",
  "dependencies": {
    "babel-loader": "^7.1.2",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "css-loader": "^0.28.8",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "glob": "^7.1.2",
    "isomorphic-unfetch": "^2.0.0",
    "js-yaml-loader": "^0.1.0",
    "mv": "^2.1.1",
    "next": "^5.0.0",
    "next-redux-wrapper": "^1.3.5",
    "node-normalize-scss": "^3.0.0",
    "node-sass": "^4.7.2",
    "postcss-cssnext": "^3.0.2",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^2.0.10",
    "raw-loader": "^0.5.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "rev-hash": "^2.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1"
  },
  "devDependencies": {
    "@storybook/react": "^3.3.9",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "commitizen": "^2.9.6",
    "corp-semantic-release": "^6.4.0",
    "cross-env": "^5.1.3",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "git-config": "0.0.7",
    "husky": "^0.14.3",
    "inquirer": "^5.1.0",
    "jest-cli": "^22.1.2",
    "validate-commit-msg": "^2.14.0",
    "webpack-bundle-analyzer": "^2.9.2"
  }
}
