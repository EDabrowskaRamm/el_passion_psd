{
  "version": "1.1.0",
  "name": "spark",
  "scripts": {
    "cm": "git-cz",
    "build": "next build",
    "start": "node server.js",
    "export": "npm run build && next export && node scripts/after-export.js",
    "lint": "eslint ./pages ./components",
    "test": "jest --no-cache --watch",
    "test-ci": "jest --coverage",
    "storybook": "npm run build && start-storybook -p 9001 -s ./ -c .storybook",
    "storybook:build": "npm run build && build-storybook -o styleguide",
    "release": "corp-semantic-release --pre-commit lint",
    "analyze": "ANALYZE=1 npm run build",
    "commitmsg": "validate-commit-msg",
    "new": "node scripts/new-project.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest-config/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest-config/assetsTransformer.js",
      "\\.(sass|scss|css|less)$": "<rootDir>/jest-config/assetsTransformer.js"
    }
  },
  "author": "Daftup",
  "dependencies": {
    "@zeit/next-sass": "^0.1.2",
    "babel-core": "6",
    "babel-plugin-inline-react-svg": "^0.5.2",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-runtime": "^6.26.0",
    "bootstrap-4-grid": "^2.4.0",
    "caniuse-lite": "^1.0.30000697",
    "cssnano": "^3.10.0",
    "file-loader": "^1.1.11",
    "isomorphic-unfetch": "^2.0.0",
    "js-yaml-loader": "^0.1.0",
    "next": "^5.1.0",
    "next-redux-wrapper": "^1.3.5",
    "node-normalize-scss": "^3.0.0",
    "node-sass": "^4.9.0",
    "postcss-cssnext": "^3.0.2",
    "raven-js": "^3.24.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "sw-precache-webpack-plugin": "^0.11.4",
    "webpack": "3.1"
  },
  "devDependencies": {
    "@storybook/react": "^3.3.9",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "commitizen": "^2.9.6",
    "corp-semantic-release": "^6.4.0",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "git-config": "0.0.7",
    "inquirer": "^5.1.0",
    "jest-cli": "^22.1.2",
    "validate-commit-msg": "^2.14.0",
    "webpack-bundle-analyzer": "^2.9.2"
  }
}
