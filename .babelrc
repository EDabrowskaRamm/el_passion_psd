{
  "presets": ["next/babel"],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["babel-plugin-root-import", {
      "paths": [{
        "rootPathSuffix": "src"
      }, {
        "rootPathPrefix": "+",
        "rootPathSuffix": "styles"
      }, {
        "rootPathPrefix": "-",
        "rootPathSuffix": "static"
      }]
    }]
  ]
}
