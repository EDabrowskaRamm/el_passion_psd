{
  "env" : {
    "all" : {
      "presets": ["next/babel"],
      "plugins": [
        "transform-decorators-legacy",
        ["babel-plugin-root-import", [{
          "rootPathSuffix": "src"
        }, {
          "rootPathPrefix": "+",
          "rootPathSuffix": "styles"
        }]]
      ]
    },
    "test": {
      "presets": [
        ["env", { "modules": "commonjs" }],
        "next/babel"
      ],
      "plugins": [
        "transform-decorators-legacy",
        ["babel-plugin-root-import", [{
          "rootPathSuffix": "src"
        }, {
          "rootPathPrefix": "+",
          "rootPathSuffix": "styles"
        }]]
      ]
    }
  }
}
