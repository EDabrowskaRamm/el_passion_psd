{
  "presets": [
    [
      "next/babel",
      {
        "@babel/preset-env": {
          "targets": {
            "chrome": "58",
            "ie": "11"
          },
          "useBuiltIns": "entry"
        }
      }
    ],
    [
      "@emotion/babel-preset-css-prop",
      {
        "autoLabel": true,
        "labelFormat": "[local]"
      }
    ]
  ],
  "plugins": [
    ["inline-react-svg"],
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["babel-plugin-root-import", {
      "paths": [{
        "rootPathSuffix": "src"
      }, {
        "rootPathPrefix": ">",
        "rootPathSuffix": "./"
      }, {
        "rootPathPrefix": "+",
        "rootPathSuffix": "styles"
      }, {
        "rootPathPrefix": "-",
        "rootPathSuffix": "static"
      }]
    }]
  ]
}
